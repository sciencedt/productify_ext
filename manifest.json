{
    "manifest_version": 3,
    "name": "Website Time Tracker",
    "version": "1.0",
    "description": "Track time spent on websites",
    "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "alarms",
      "notifications"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon-min.png",
        "48": "icon-min.png",
        "128": "icon-min.png"
      }
    }
  }